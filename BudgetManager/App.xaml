<Application x:Class="BudgetManager.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:BudgetManager"
             StartupUri="Views\WelcomeView.xaml">
    <Application.Resources>

        <ResourceDictionary>
            <BitmapImage x:Key="budget" UriSource="Backgrounds\budget.jpg"/>
            <BitmapImage x:Key="pieIcon" UriSource="Icons\chart-pie-gray.ico"/>
            <BitmapImage x:Key="pie" UriSource="Icons\chart-pie.png"/>
            <BitmapImage x:Key="trends" UriSource="Icons\finance.png"/>
            <BitmapImage x:Key="priorities" UriSource="Icons\triangle.png"/>
            <BitmapImage x:Key="back" UriSource="Icons\arrow-left.png"/>
            <BitmapImage x:Key="bank" UriSource="Icons\bank.png"/>
            <BitmapImage x:Key="calculator" UriSource="Icons\calculator.png"/>
            <BitmapImage x:Key="savings" UriSource="Backgrounds\savings.png"/>
            <BitmapImage x:Key="printer" UriSource="Icons\printing.png"/>
        </ResourceDictionary>

    </Application.Resources>
</Application>
